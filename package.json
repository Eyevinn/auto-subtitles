{
  "name": "@eyevinn/typescript-nodejs",
  "version": "1.0.0",
  "description": "The default typescript-nodejs template for eyevinn projects",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint .",
    "pretty": "prettier --check --ignore-unknown .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
